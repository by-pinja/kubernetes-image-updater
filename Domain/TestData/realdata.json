{
    "apiVersion": "v1",
    "items": [
        {
            "apiVersion": "extensions/v1beta1",
            "kind": "Deployment",
            "metadata": {
                "labels": {
                    "app": "authorization-master",
                    "version": "master"
                },
                "name": "authorization-master",
                "namespace": "authorization"
            },
            "spec": {
                "replicas": 1,
                "selector": {
                    "matchLabels": {
                        "app": "authorization-master",
                        "version": "master"
                    }
                },
                "template": {
                    "metadata": {
                        "labels": {
                            "app": "authorization-master",
                            "version": "master"
                        },
                        "name": "authorization-master"
                    },
                    "spec": {
                        "containers": [
                            {
                                "image": "eu.gcr.io/ptcs-docker-registry/authorization:latest-master",
                                "imagePullPolicy": "Always",
                                "name": "authorization-master"
                            }
                        ]
                    }
                }
            }
        },
        {
            "apiVersion": "extensions/v1beta1",
            "kind": "Deployment",
            "metadata": {
                "labels": {
                    "app": "beertapped-master",
                    "version": "master"
                },
                "name": "beertapped-master",
                "namespace": "beertapped"
            },
            "spec": {
                "replicas": 1,
                "selector": {
                    "matchLabels": {
                        "app": "beertapped-master",
                        "version": "master"
                    }
                },
                "template": {
                    "metadata": {
                        "labels": {
                            "app": "beertapped-master",
                            "version": "master"
                        },
                        "name": "beertapped-master"
                    },
                    "spec": {
                        "containers": [
                            {
                                "image": "eu.gcr.io/ptcs-docker-registry/beertapped:master-1",
                                "imagePullPolicy": "Always",
                                "name": "beertapped-master"
                            }
                        ]
                    }
                }
            }
        }
    ]
}
